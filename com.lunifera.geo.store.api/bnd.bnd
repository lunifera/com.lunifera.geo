Bundle-Version: 0.3.0.${tstamp}
Bundle-Description: 				\
	API for the Lunifera Geo store.\

	
Export-Package: \
	com.lunifera.geo.api,\
	com.lunifera.geo.store.api;provide:=true,\
	com.lunifera.geo.store.api.dto,\
	com.lunifera.geo.store.api.query

Require-Capability: \
	compile-only

-buildpath: \
	osgi.enroute.base.api;version=1.0,\
	com.lunifera.geo.api;version=latest

-testpath: \
	osgi.enroute.junit.wrapper;version=4.12
