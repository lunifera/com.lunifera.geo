#
#  BUNDLE 
#


Bundle-Version: 0.3.0.${tstamp}
Bundle-Description: 				\
	A bundle with a provider. Notice that this provider exports the API package. \
	It also provides a JUnit test and it can be run standalone. \

Private-Package:  \
	com.lunifera.geo.jgeohash.provider
	
-buildpath: \
	osgi.enroute.base.api;version=1.0,\
	com.lunifera.geo.api;version=latest,\
	com.lunifera.wrapping.jgeohash.provider;version=latest

-testpath: \
	osgi.enroute.junit.wrapper;version=4.12

-includeresource: {readme.md}

-runrequires: \
	osgi.identity;filter:='(osgi.identity=com.lunifera.geo.jgeohash.provider)',\
	osgi.identity;filter:='(&(osgi.identity=org.osgi.service.event)(version>=1.3.1))',\
	osgi.identity;filter:='(osgi.identity=org.apache.felix.gogo.shell)',\
	osgi.identity;filter:='(osgi.identity=org.apache.felix.gogo.command)',\
	osgi.identity;filter:='(osgi.identity=osgi.enroute.webconsole.xray.provider)',\
	osgi.identity;filter:='(&(osgi.identity=slf4j.api)(version>=1.7.2))',\
	osgi.identity;filter:='(&(osgi.identity=osgi.enroute.logger.simple.provider)(version>=1.1.1))',\
	osgi.identity;filter:='(&(osgi.identity=biz.aQute.bndlib)(version>=3.0.0))'
	
-runbundles: \
	biz.aQute.bndlib;version='[3.0.0,3.0.1)',\
	com.lunifera.geo.jgeohash.provider;version=snapshot,\
	com.lunifera.wrapping.jgeohash.provider;version=snapshot,\
	com.springsource.org.json;version='[1.0.0,1.0.1)',\
	log4j;version='[1.2.17,1.2.18)',\
	org.apache.commons.fileupload;version='[1.3.1,1.3.2)',\
	org.apache.commons.io;version='[2.4.0,2.4.1)',\
	org.apache.felix.configadmin;version='[1.8.6,1.8.7)',\
	org.apache.felix.gogo.command;version='[0.14.0,0.14.1)',\
	org.apache.felix.gogo.runtime;version='[0.16.2,0.16.3)',\
	org.apache.felix.gogo.shell;version='[0.10.0,0.10.1)',\
	org.apache.felix.http.api;version='[3.0.0,3.0.1)',\
	org.apache.felix.http.jetty;version='[3.1.0,3.1.1)',\
	org.apache.felix.http.servlet-api;version='[1.1.2,1.1.3)',\
	org.apache.felix.log;version='[1.0.1,1.0.2)',\
	org.apache.felix.scr;version='[2.0.0,2.0.1)',\
	org.apache.felix.webconsole;version='[4.2.8,4.2.9)',\
	org.eclipse.equinox.coordinator;version='[1.3.100,1.3.101)',\
	org.eclipse.equinox.event;version='[1.3.100,1.3.101)',\
	org.eclipse.equinox.metatype;version='[1.4.100,1.4.101)',\
	org.osgi.service.event;version='[1.3.1,1.3.2)',\
	org.osgi.service.metatype;version='[1.3.0,1.3.1)',\
	osgi.enroute.bostock.d3.webresource;version='[3.5.6,3.5.7)',\
	osgi.enroute.dto.bndlib.provider;version='[1.0.0,1.0.1)',\
	osgi.enroute.executor.simple.provider;version='[1.0.0,1.0.1)',\
	osgi.enroute.logger.simple.provider;version='[1.1.1,1.1.2)',\
	osgi.enroute.web.simple.provider;version='[1.3.3,1.3.4)',\
	osgi.enroute.webconsole.xray.provider;version='[2.0.0,2.0.1)',\
	slf4j.api;version='[1.7.5,1.7.6)'
	
-includeresource: \
	
	
Export-Package: \
	com.lunifera.geo.api,\
	com.lunifera.geo.api.dto
Test-Cases: com.lunifera.geo.jgeohash.provider.JgeohashImplTest
-runproperties: \
	-console=,\
	-consoleLog=